  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  }
}
